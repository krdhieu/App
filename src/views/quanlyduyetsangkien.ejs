<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="/scss/styles.scss">
    <title>Quản lý sáng kiến</title>
</head>

<body>
    <div>
        <h1>Danh sách sáng kiến</h1>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">STT</th>
                    <th scope="col">Tên sáng kiến</th>
                    <th scope="col">Mục tiêu</th>
                    <!-- <th scope="col">Nội dung</th> -->
                    <th scope="col">Lợi ích</th>
                    <th scope="col">Đối tượng</th>
                    <!-- <th scope="col">Đợt</th> -->
                    <th scope="col">Trạng thái</th>
                    <!-- <th scope="col">Đính kèm</th> -->
                    <th scope="col">Tác vụ</th>
                </tr>
            </thead>
            <tbody>
                <% for( let i=0 ; i < dataSangkien.length; i++){ %>
                    <tr>
                        <th scope="row">
                            <%= i +1 %>
                        </th>
                        <td>
                            <%= dataSangkien[i].tensangkien %>
                        </td>
                        <td>
                            <%= dataSangkien[i].muctieu %>
                        </td>
                        <td>
                            <%= dataSangkien[i].loiich %>
                        </td>
                        <td>
                            <%= dataSangkien[i].doituong %>
                        </td>
                        <td>
                            <%= dataSangkien[i].tentrangthai %>
                        </td>
                        <td><a href="/quanlysangkien/duyet/<%= dataSangkien[i].masangkien %>" target="_self">Duyệt</a>
                            <a href="/quanlysangkien/huy/<%= dataSangkien[i].masangkien %>" target="_self">Hủy</a>
                        </td>
                    </tr>
                    <% } %>
            </tbody>
        </table>

        <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
            integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
            crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"
            integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
            crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"
            integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
            crossorigin="anonymous"></script>
    </div>
</body>

</html>