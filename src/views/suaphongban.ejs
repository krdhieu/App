<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous" />
    <link rel="stylesheet" href="/scss/styles.scss" />
    <title>Sửa phòng ban</title>
</head>

<body>
    <div>
        <form action="/uploadphongban" method="post">
            <!-- Vertical -->
            <div class="form-group">
                <label>Mã phòng ban</label>
                <input type="text" name="maphongban" class="form-control" value="<%= dataPhongban.maphongban %>"
                    readonly />
                <label>Tên Phòng Ban</label>
                <input type="text" name="tenphongban" class="form-control" value="<%= dataPhongban.tenphongban %>"
                    required />
                <label>Năm thành lập</label>
                <input type="date" name="namthanhlap" id="namthanhlap" class="form-control"
                    value="<%=moment(dataPhongban.ngaythanhlap).format( 'YYYY-MM-DD' )%>" required />
                <label>Mô tả phòng ban</label>
                <textarea name="motaphongban" class="form-control" id="" cols="30"
                    rows="10"><%= dataPhongban.motaphongban %></textarea>
                <button type=" submit" class="btn btn-primary">
                    Submit</button>
            </div>
        </form>

        <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
            integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
            crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"
            integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
            crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"
            integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
            crossorigin="anonymous"></script>
    </div>
</body>

</html>