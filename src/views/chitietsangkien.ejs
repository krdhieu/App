<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="/scss/styles.scss">
    <title>Chi tiet sang kien</title>
</head>

<body>
    <div class="container">
        <label>tensangkien</label>
        <input type="text" name="tensangkien" class="form-control" value="<%= dataSangkien.tensangkien %>">
        <label for="">nguoi chu tri</label>
        <input type="text" name="chutri" class="form-control" value="<% %>">
        <label for="">nguoi tro ly</label>
        <input type="text" name="troly" class="form-control" value="<% %>">
        <label for="">muc tieu</label>
        <input type="text" name="muctieu" class="form-control" value="<%= dataSangkien.muctieu %>">
        <label for="">muc tieu</label>
        <textarea name="noidung" id="" class="form-control" cols="30"
            rows="10"><%= dataSangkien.tensangkien %></textarea>
        <label for="">loi ich</label>
        <input type="text" name="loiich" class="form-control" value="<%= dataSangkien.loiich %>">
        <label for="">doi tuong</label>
        <input type="text" name="doituong" class="form-control" value="<%= dataSangkien.doituong %>">

        <div class="uploadfile">
            <form method="post" action="/upload-file" enctype="multipart/form-data">
                <div>
                    <label for="">Upload: </label>
                    <input type="file" name="profile_file">
                </div>
                <div>
                    <button type="submit">Save me</button>
                </div>
            </form>
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
        integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"
        integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"
        integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
        crossorigin="anonymous"></script>
    </div>
</body>

</html>